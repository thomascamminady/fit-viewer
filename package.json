{
  "name": "fit-viewer",
  "displayName": "FIT File Viewer",
  "description": "A VSCode extension to preview .fit files",
  "version": "0.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomascamminady/fit-viewer"
  },
  "icon": "icon.png",
  "publisher": "thomascamminady",
  "author": "Thomas Camminady",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "fitViewer.fitEditor",
        "displayName": "FIT File Viewer",
        "selector": [
          {
            "filenamePattern": "*.fit"
          }
        ]
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./ && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 media/* out/media/",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.96.0",
    "@vscode/vsce": "^2.19.0",
    "typescript": "^5.7.3",
    "copyfiles": "^2.4.1"
  },
  "dependencies": {
    "@garmin/fitsdk": "^21.161.0",
    "arquero": "^7.2.0"
  }
}